record(calc,"$(P)$(AXIS_NAME)-PosActSim")
{
  field(DESC, "Calc simulated act. pos. of axis 1")
  field(PINI, "$(PINI=1)")
  field(CALC, "A*0.95")
  field(INPA, "$(P)$(AXIS_NAME)-PosAct CP")
  field(FLNK, "$(P)$(AXIS_NAME)-PosActSim.PROC")
  field(LOW,  "-50")
  field(LOLO, "-60")
  field(HIGH, "50")
  field(HIHI, "60")
  field(HYST, "0")
  field(LLSV, "MAJOR")
  field(LSV,  "MINOR")
  field(HSV,  "MINOR")
  field(HHSV, "MAJOR")
}
