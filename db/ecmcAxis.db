}

record(mbbiDirect,"$(P)$(AXIS_NAME)-Stat_"){
  field(DESC, "$(AXIS_NAME): Status Word mbbiD")
  field(PINI, "$(PINI=1)")
  field(DTYP, "asynUInt32Digital")
